{
    "name": "edyan/mysqldiff",
    "description": "A simple tool that helps to compare 2 databases using Doctrine DBAL.",
    "autoload": {
        "psr-4": {
            "Edyan\\MysqlDiff\\": "src/"
        }
    },
    "keywords": ["mysql", "database", "diff"],
    "type": "project",
    "authors": [
        {
            "name": "Emmanuel Dyan",
            "email": "emmanuel.dyan@inetprocess.com"
        }
    ],
    "license": "GPL-2.0",
    "require": {
        "php": ">=5.4",
        "easybook/geshi": "@stable",
        "jdorn/sql-formatter": "^1.2",
        "doctrine/dbal": "~2.5",
        "silex/silex": "~1.3",
        "symfony/config": "~2.7",
        "symfony/form": "~2.7",
        "symfony/locale": "~2.7",
        "symfony/security-csrf": "~2.7",
        "symfony/translation": "~2.7",
        "symfony/twig-bridge": "~2.7",
        "symfony/validator": "~2.7",
        "symfony/var-dumper": "~2.7",
        "symfony/yaml": "~2.7"
    },
    "suggest": {
        "fabpot/php-cs-fixer": "Fix PSR2 and other coding style issues"
    },
    "require-dev": {
        "symfony/browser-kit": "^2.7",
        "symfony/css-selector": "^2.7"
    }
}
